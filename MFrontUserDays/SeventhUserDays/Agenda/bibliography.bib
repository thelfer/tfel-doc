@article{ricaud_effective_2009,
	title = {Effective properties of linear viscoelastic heterogeneous media: Internal variables formulation and extension to ageing behaviours},
	volume = {46},
	issn = {0020-7683},
	url = {https://www.sciencedirect.com/science/article/pii/S0020768308005027},
	doi = {10.1016/j.ijsolstr.2008.12.007},
	shorttitle = {Effective properties of linear viscoelastic heterogeneous media},
	abstract = {The Laplace–Carson transform classically used for homogenization of linear viscoelastic heterogeneous media yields integral formulations of effective behaviours. These are far less convenient than internal variables formulations with respect to computational aspects as well as to theoretical extensions to closely related problems such as ageing viscoelasticity. Noticing that the collocation method is usually adopted to invert the Laplace–Carson transforms, we first remark that this approximation is equivalent to an internal variables formulation which is exact in some specific situations. This result is illustrated for a two-phase composite with phases obeying a compressible maxwellian behaviour. Next, an incremental formulation allows to extend at each time step the previous general framework to ageing viscoelasticity. Finally, with the help of a creep test of a porous viscoelastic matrix reinforced with elastic inclusions, it is shown that the method yields accurate predictions (comparing to reference results provided by periodic cell finite element computations).},
	pages = {1599--1606},
	number = {7},
	journaltitle = {International Journal of Solids and Structures},
	shortjournal = {International Journal of Solids and Structures},
	author = {Ricaud, Jean-Marc and Masson, Renaud},
	urldate = {2021-08-26},
	date = {2009-04-01},
	langid = {english},
	keywords = {Ageing viscoelasticity, Collocation method, Hashin–Shtrikman, Heterogeneous media, Internal variables},
	file = {ScienceDirect Full Text PDF:/home/th202608/.zotero/zotero/vzpzva46.default/zotero/storage/LCIGXRGM/Ricaud et Masson - 2009 - Effective properties of linear viscoelastic hetero.pdf:application/pdf;ScienceDirect Full Text PDF:/home/th202608/.zotero/zotero/vzpzva46.default/zotero/storage/AXIYVY5Z/Ricaud et Masson - 2009 - Effective properties of linear viscoelastic hetero.pdf:application/pdf;ScienceDirect Snapshot:/home/th202608/.zotero/zotero/vzpzva46.default/zotero/storage/75E9BZFF/S0020768308005027.html:text/html;ScienceDirect Snapshot:/home/th202608/.zotero/zotero/vzpzva46.default/zotero/storage/AHNXSTHZ/S0020768308005027.html:text/html}
}

@article{meric_fe_1994,
	title = {F.E. calculations of copper bicrystal specimens submitted to tension-compression tests},
	volume = {42},
	issn = {0956-7151},
	url = {http://www.sciencedirect.com/science/article/pii/0956715194902879},
	doi = {10.1016/0956-7151(94)90287-9},
	abstract = {Tension-compression tests are performed on copper bicrystalline specimens, with a grain boundary parallel to the tension axis. The characterisation of the slip system is made from microscopical observations at the surface of the specimens. A crystalline viscoplasticity model is identified on the single crystal tests. The model is then used in a Finite Element code to predict the behaviour of the bicrystals. The numerical simulation is in good agreement with the experimental global force-displacement curve, and provides also a good description of the local slip activity.
Résumé
Des essais de traction-compression sont réalisés sur des bicristaux de cuivre dont le joint est parallèle à l'axe de traction. La caractérisation des systèmes de glissement actifs est effectuée par des observations au microscope de la surface des éprouvettes. Un modèle de viscoplasticité cristalline est identifié sur les essais de monocristal, puis utilisé dans un programme de calcul par éléments finis pour prévoir le comportement du bicristal. La simulation numérique est en bon accord avec la courbe globale force-déplacement, et procure aussi une bonne description des systèmes de glissement actifs.
Zusammenfassung
Druck- und Zugversuche wurden an Bikristallen aus Kupfer durchgeführt, deren Grenze parallel zur Zugachse verlief. Die Gleitsysteme werden durch mikroskopische Untersuchungen der Probenoberfläche ermittelt. Ein viskoplastiches Modell unter Berücksichtigung der Gleitsysteme wird ausgehend von Versuchen an Einkristallen erstellt, und wird in einem {FE}-Programm verwendet, um das Verhalten des Bikristalles zu bestimmen. Die numerische Simulation stimmt weitgehend mit dem Maschinendiagramm (Kraft- Verlängerung der Probe) überein, und liefert auch eine befriedigende Beschreibung der aktiven Gleitsysteme.},
	pages = {921--935},
	number = {3},
	journaltitle = {Acta Metallurgica et Materialia},
	shortjournal = {Acta Metallurgica et Materialia},
	author = {Méric, L. and Cailletaud, G. and Gaspérini, M.},
	urldate = {2020-10-02},
	date = {1994-03-01},
	langid = {english},
	file = {ScienceDirect Snapshot:/home/th202608/.zotero/zotero/vzpzva46.default/zotero/storage/YKW78RG2/0956715194902879.html:text/html}
}

